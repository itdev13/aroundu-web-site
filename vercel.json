{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/assetlinks.json",
      "dest": "/.well-known/assetlinks.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/.well-known/apple-app-site-association",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/privacy/?",
      "dest": "/privacy.html"
    },
    {
      "src": "/terms/?",
      "dest": "/terms.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}